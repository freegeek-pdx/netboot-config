# this file goes in whatever directory the PXE boot menu is in, for example:
# /var/lib/tftpboot/pxelinux.cfg/default

default menu.c32
prompt 0

label desktop
  menu label Desktop Build
  kernel menu.c32
  append pxelinux.cfg/desktop

label laptop
  menu label Laptop Build
  kernel menu.c32
  append pxelinux.cfg/laptop

label server
  menu label Server Build
  kernel menu.c32
  append pxelinux.cfg/server

label installers
  menu label Installers
  kernel menu.c32
  append pxelinux.cfg/installers

label live-boot
  menu label Live Boot
  kernel menu.c32
  append pxelinux.cfg/live-boot

INCLUDE pxelinux.cfg/diskless-terminal 
#label diskless-terminal
#  menu label Diskless Terminal
#  kernel menu.c32
#  append pxelinux.cfg/diskless-terminal

label hardware-testing
  menu label Hardware Testing
  kernel menu.c32
  append pxelinux.cfg/hardware-testing

label other
  menu label Other
  kernel menu.c32
  append pxelinux.cfg/other

menu begin should-be-hidden
  menu label Hidden Menu Entries

INCLUDE pxelinux.cfg/printme

INCLUDE pxelinux.cfg/live-boot

INCLUDE pxelinux.cfg/debian

INCLUDE pxelinux.cfg/ubuntu

menu end
