label ltsp-wheezy-fatclient
  menu label Diskless Terminal (experimental)
  kernel /ltsp/i386-wheezy/vmlinuz
  append ro initrd=/ltsp/i386-wheezy/initrd.img init=/sbin/init-ltsp quiet root=/dev/nfs ip=dhcp boot=nfs nfsroot=/opt/ltsp/i386-wheezy/ fatclient=true
  ipappend 2

INCLUDE pxelinux.cfg/default
