INCLUDE /pxelinux.cfg/main-menu

  label debian-live-rescue-techsupport
    menu label Debian Live Rescue
    menu default
    kernel ifcpu64.c32
    append debian-live-rescue-amd64 -- debian-live-rescue-i386
    text help
      64-bit if supported, or 32-bit if not
    endtext

    label live-techsupport-amd64
      menu label Experimental Tech Support Debian Live Rescue (amd64)
      kernel live/live-techsupport-amd64/live/vmlinuz
      initrd live/live-techsupport-amd64/live/initrd.img
      append boot=live config quiet netboot=nfs nfsroot=/srv/nfs/live-techsupport-amd64 live-config.username=techsupport

INCLUDE /pxelinux.cfg/installer-include
