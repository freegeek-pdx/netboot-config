#debian

label squeeze-server
  menu label Install Debian 6.0 Squeeze (32/64) (FG Server)
  kernel ifcpu64.c32
  append squeeze64-server -- squeeze32-server
  text help
    Install preseeded Squeeze 64-bit if supported, or 32-bit if not
  endtext

